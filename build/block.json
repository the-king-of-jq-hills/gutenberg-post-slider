{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/txp-slider",
  "version": "0.1.0",
  "title": "Txp Slider",
  "category": "media",
  "icon": "cover-image",
  "description": "Post Featured Image Slider Block",
  "supports": {
    "html": false
  },
  "textdomain": "txp-slider",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "txp-default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "txp-outline",
      "label": "Outline"
    },
    {
      "name": "txp-rounded",
      "label": "Rounded"
    }
  ],
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "align": {
      "type": "string",
      "default": "none"
    },
    "backgroundColor": {
      "type": "string",
      "default": "#f2a42e"
    },
    "textColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "siteURL": {
      "type": "string",
      "default": ""
    },
    "theImage": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "attributes": {
      "content": "Sample Text For Sample Block!",
      "align": "center",
      "affiliateLink": "https://www.google.com/",
      "linkLabel": "Exaple Link"
    }
  }
}